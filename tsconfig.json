{
	"compilerOptions": {
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"target": "es2020",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"lib": [
			"es2020"
		],
		"rootDir": "src",
		"outDir": "out"
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}